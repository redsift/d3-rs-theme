{
  "name": "@redsift/d3-rs-theme",
  "version": "0.6.0",
  "description": "Package for the Red Sift default color palette, fonts, curves and patterns.",
  "keywords": [
    "color",
    "pattern",
    "redsift"
  ],
  "homepage": "https://github.com/redsift/d3-rs-theme",
  "license": "MIT",
  "author": {
    "name": "Rahul Powar",
    "url": "https://medium.com/@rahulpowar"
  },
  "main": "distribution/d3-rs-theme.umd-es2015.min.js",
  "jsnext:main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/redsift/d3-rs-theme.git"
  },
  "scripts": {
    "serve": "gulp -o d3-rs-theme serve",
    "prepublish": "npm run clean; npm run build",
    "build": "npm run clean; node ./node_modules/@redsift/rollup-bundler/src/index.js bundle.config.js",
    "clean": "rm -rf dist",
    "pretest": "npm outdated && npm run build",
    "test": "tape 'test/**/*-test.js' && eslint index.js src"
  },
  "dependencies": {
    "bezier-easing": "^2.0.3"
  },
  "devDependencies": {
    "@redsift/rollup-bundler": "^2.0.2",
    "@redsift/tape-reel": "~0.1.1",
    "seedrandom": "^2.4.2",
    "browser-sync": "^2.13.0",
    "del": "^2.2.1",
    "eslint": "^3.0.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "yargs": "^4.8.0"
  },
  "overrides": {
    "lodash": "4.17.21",
    "lodash.template": "4.5.0",
    "chokidar": "3.5.3",
    "glob-parent": "6.0.2"
  }
}
